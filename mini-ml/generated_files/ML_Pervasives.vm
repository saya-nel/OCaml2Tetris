///////////////////////////////////////////////////////////////
function ML_Pervasives.failwith 0
    push argument 0
    call ML_Pervasives.print_string 1
    pop temp 0
    push constant 0
    call ML_Pervasives.print_newline 1
    pop temp 0
    push constant 1
    call ML_Pervasives.exit 1
    return
