///////////////////////////////////////////////////////////////
function ML_Pervasives.exit 0
    push argument 0
    call ML_Internal.exit 1
    return
///////////////////////////////////////////////////////////////
function ML_Pervasives.ref 0
    push constant 1
    push argument 0
    call ML_Internal.array_make 2
    call ML_Internal.obj_magic 1
    return
///////////////////////////////////////////////////////////////
function ML_Pervasives.ref_contents 0
    push argument 0
    call ML_Internal.obj_magic 1
    push constant 0
    call ML_Internal.array_get 2
    return
///////////////////////////////////////////////////////////////
function ML_Pervasives.ref_set_contents 0
    push argument 0
    call ML_Internal.obj_magic 1
    push constant 0
    push argument 1
    call ML_Internal.array_set 3
    return
///////////////////////////////////////////////////////////////
function ML_Pervasives.incr 0
    push argument 0
    push argument 0
    call ML_Pervasives.ref_contents 1
    push constant 1
    add
    call ML_Pervasives.ref_set_contents 2
    return
///////////////////////////////////////////////////////////////
function ML_Pervasives.decr 0
    push argument 0
    push argument 0
    call ML_Pervasives.ref_contents 1
    push constant 1
    add
    call ML_Pervasives.ref_set_contents 2
    return
